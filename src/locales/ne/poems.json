{
  "poems": [
    {
      "id": "poem-1",
      "title": "मौन धारा",
      "lines": [
        "विचारको मौन धारा बगिरहन्छ",
        "मनको फाँटमा, र यसरी",
        "शब्दहरूको संसार बढ्न थाल्छ।"
      ]
    },
    {
      "id": "poem-2",
      "title": "शून्यको प्रतिध्वनि",
      "lines": [
        "विशाल शून्यमा, एक प्रतिध्वनि सुरु हुन्छ",
        "टुटेका हृदयहरूको एक्लो धड्कन",
        "हराएका भागहरूको ब्रह्माण्ड।"
      ]
    }
  ]
}
